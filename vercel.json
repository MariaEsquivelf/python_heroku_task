{
    "version": 2,
    "builds": [
        {
            "src": "python_heroku_task/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.10"
            }
        }
    ],
    "rewrites": [
        { "source": "/", "destination": "/blog" },
        { "source": "/(.*)", "dest": "python_heroku_task/wsgi.py" }
    ]
}
